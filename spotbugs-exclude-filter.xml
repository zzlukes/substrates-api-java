<FindBugsFilter>

  <Match>
    <Or>
      <!-- Ignore checking for generated JMH benchmarking classes -->
      <Package name="~.*\.jmh_generated"/>
    </Or>
  </Match>
  <Match>
    <Bug pattern="NM_SAME_SIMPLE_NAME_AS_INTERFACE"/>
  </Match>
  <Match>
    <Bug pattern="EQ_COMPARETO_USE_OBJECT_EQUALS"/>
  </Match>

</FindBugsFilter>